{
  "name": "frontend-starter",
  "version": "1.0.0",
  "description": "Frontend Starter Kit",
  "scripts": {
    "start": "NODE_PATH=. NODE_ENV=development node_modules/.bin/babel-node server.js --verbose",
    "start:production": "NODE_PATH=. NODE_ENV=production npm run build && pm2 start build.js --name \"hellohome\"",
    "test": "npm run lint",
    "build": "npm run build:server && npm run build:client",
    "build:server": "node_modules/.bin/webpack --config webpack.server.js --progress",
    "build:client": "node_modules/.bin/webpack --config webpack.client.js --progress",
    "lint": "node_modules/.bin/eslint ."
  },
  "author": "Anton Drobot",
  "license": "ISC",
  "dependencies": {
    "bytes": "^2.5.0",
    "cat-log": "^1.0.2",
    "dotenv": "^4.0.0",
    "koa": "^2.2.0",
    "koa-bodyparser": "^4.2.0",
    "koa-helmet": "^3.1.0",
    "koa-static": "^3.0.0",
    "lodash": "^4.17.4",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "ms": "^1.0.0",
    "node-exceptions": "^2.0.1",
    "path-to-regexp": "^1.7.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.7",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-async-to-generator": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "node-sass": "^4.5.2",
    "node-sass-import-once": "git+https://git@github.com/anton-drobot/node-sass-import-once.git",
    "postcss-calc": "^5.3.1",
    "postcss-flexbugs-fixes": "^2.1.1",
    "postcss-loader": "^1.3.3",
    "prop-types": "^15.5.9",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "uglifyjs-webpack-plugin": "^0.4.2",
    "webpack": "^2.3.3",
    "webpack-koa2-middleware": "^1.0.1"
  }
}
